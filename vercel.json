{
  "functions": {
    "api/app.py": {
      "runtime": "python3.9",
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/api/static/$1"
    },
    {
      "source": "/templates/(.*)",
      "destination": "/api/templates/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/app.py"
    }
  ]
}


